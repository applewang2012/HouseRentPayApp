<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
     >
	<LinearLayout 
	    android:id="@+id/id_frament_house_cotent"
	    android:layout_width="match_parent"
   		android:layout_height="match_parent"
   		android:orientation="vertical"
	    >
	    <ImageView 
        	android:layout_width="320dp"
	        android:layout_height="130dp"
	        android:layout_marginTop="10dp"
	        android:layout_gravity="center_horizontal"
	        android:background="@drawable/view_banner"
	        />
	    <ListView
            android:id="@+id/id_surround_listview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:divider="#55000000"
			android:dividerHeight="1dp"
            android:listSelector="#00000000" />
	    
	</LinearLayout>
    <TextView 
        android:id="@+id/id_frament_house_no_cotent"
        android:layout_width="match_parent"
    	android:layout_height="match_parent"
    	android:text="未搜索到任何数据"
    	android:gravity="center"
    	android:textSize="28sp"
    	android:textColor="#000000"
    	android:visibility="invisible"
        />
	<include
        android:id="@+id/id_data_loading"
        layout="@layout/layout_loading_dialog"
       />   
</FrameLayout>
