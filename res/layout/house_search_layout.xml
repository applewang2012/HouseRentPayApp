<FrameLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" 
    android:background="#ffffff"
    android:orientation="vertical"
    android:padding="3dp"
    >
    <LinearLayout 
	    android:layout_width="match_parent"
	    android:layout_height="match_parent" 
	    android:background="#ffffff"
	    android:orientation="vertical"
	    >
	    
	   
	        <!-- <EditText 
		        android:id="@+id/id_input_address"
		        android:layout_width="0dp"
			    android:layout_height="match_parent"
			    android:layout_weight="4"
			    android:hint="请输入详细地址进行搜索"
			    android:gravity="left|center_vertical"
			    android:paddingLeft="3dp"
			    android:textSize="14dp"
			    android:background="@drawable/item_shape_no_solid_corner"
			    android:textColor="#afaeae"
		        /> -->
	        <TextView 
	            android:id="@+id/id_show_search_content"
				android:layout_width="@dimen/base_dimen_660"
			    android:layout_height="35dp"
			    android:layout_marginTop="@dimen/base_dimen_20"
			    android:text="搜   索"
			    android:textColor="#999999"
			    android:textSize="@dimen/base_dimen_35"
			    android:gravity="center"
			    android:layout_gravity="center_horizontal"
			    android:background="@drawable/item_shape_no_solid_corner_press"
	            />
	        
	    
	    
	    <tenant.guardts.house.search.ExpandMenuView
	        android:id="@+id/expandTabView"
	        android:layout_marginTop="3dp"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:divider="#362e2b"
	        />
	    
	    <ListView 
	        android:id="@+id/listView"
	        android:layout_width="fill_parent"
		    android:layout_height="match_parent"
		    android:cacheColorHint="#00000000"
		    android:fadingEdge="none"
		    android:divider="#afaeae"
		    android:paddingLeft="2dp"
		    android:paddingRight="2dp"
		    android:dividerHeight="1dp"
	        />
		  
	</LinearLayout>
	<TextView 
	        android:id="@+id/id_frament_house_no_cotent"
	        android:layout_width="match_parent"
	    	android:layout_height="match_parent"
	    	android:text="暂无租房信息"
	    	android:gravity="center"
	    	android:textSize="28sp"
	    	android:textColor="#000000"
	    	android:visibility="invisible"
	        />
		<include
	        android:id="@+id/id_data_loading"
	        layout="@layout/layout_loading_dialog"
	       /> 
</FrameLayout>
