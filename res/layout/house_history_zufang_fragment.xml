<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f1f2f3" >

    <ListView
        android:id="@+id/id_fragment_house_listview"
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:background="#f1f2f3"
        android:cacheColorHint="#00000000"
        android:divider="#00000000"
        android:dividerHeight="@dimen/base_dimen_10"
        android:fadingEdge="none"
      />

    <TextView
        android:id="@+id/id_frament_house_no_cotent"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:text="暂无租房历史"
        android:textColor="#888888"
        android:textSize="@dimen/base_dimen_30"
        android:visibility="invisible" />

   
    <include
        android:id="@+id/id_data_loading"
        android:visibility="invisible"
        layout="@layout/layout_loading_dialog" />

</FrameLayout>