                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#0c000000" />

                <LinearLayout
                   android:layout_width="match_parent"
                    android:layout_height="@dimen/base_dimen_90"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="@dimen/base_dimen_35"
                	android:layout_marginRight="@dimen/base_dimen_35"
                    android:orientation="horizontal" >
                    
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="left|center_vertical"
                            android:text="姓	       名"
                            android:textColor="#000000"
                            android:textSize="@dimen/base_dimen_30" />


                    <EditText
                        android:id="@+id/id_rent_house_name"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="#00000000"
                        android:gravity="center_vertical"
                        android:hint="请输入租房人姓名"
                        android:layout_marginLeft="@dimen/base_dimen_50"
                        android:textColor="#444444"
                        android:textColorHint="#b2b2b2"
                        android:textSize="@dimen/base_dimen_30" />

                    
                </LinearLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#0c000000" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/base_dimen_90"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="@dimen/base_dimen_35"
                	android:layout_marginRight="@dimen/base_dimen_35"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:text="手	       机"
                        android:textColor="#000000"
                        android:textSize="@dimen/base_dimen_30" />

                    <EditText
                        android:id="@+id/id_rent_house_phone"
                       android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="#00000000"
                        android:gravity="center_vertical"
                        android:layout_marginLeft="@dimen/base_dimen_50"
                        android:hint="请输入租房人手机号码"
                        android:inputType="number"
                        android:maxLength="11"
                        android:textColor="#444444"
                        android:textColorHint="#b2b2b2"
                        android:textSize="@dimen/base_dimen_30" />
                </LinearLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#0c000000" />
                
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/base_dimen_90"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="@dimen/base_dimen_35"
                	android:layout_marginRight="@dimen/base_dimen_35"
                    android:orientation="horizontal" >

                    <TextView
                       android:layout_width="wrap_content"
                       android:layout_height="match_parent"
                       android:gravity="left|center_vertical"
                       android:text="验  证  码"
                        android:textColor="#000000"
                        android:textSize="@dimen/base_dimen_30" />
					<FrameLayout 
                       android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="#00000000"
                        android:gravity="center_vertical"
                        android:layout_marginLeft="@dimen/base_dimen_50"
                        android:orientation="horizontal"
					    >

					    <EditText
					        android:id="@+id/id_rent_house_phone_verify"
					        android:layout_width="wrap_content"
                        	android:layout_height="match_parent"
                        	android:background="#00000000"
                        	android:gravity="center_vertical"
					        android:hint="请输入手机验证码"
					        android:inputType="number"
					        android:maxLength="6"
					        android:textColor="#444444"
					        android:textColorHint="#b2b2b2"
					        android:textSize="@dimen/base_dimen_30" />


					    <TextView
					        android:id="@+id/id_rent_house_get_verifycode"
					        android:layout_width="wrap_content"
					        android:layout_height="match_parent"
					        android:layout_marginRight="@dimen/base_dimen_15"
					        android:clickable="true"
					        android:layout_gravity="center_vertical|right"
					        android:gravity="center"
					        android:text="获取验证码"
					        android:textColor="#337ffd"
					        android:textSize="@dimen/base_dimen_28" />

					</FrameLayout>
                    
        </LinearLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#0c000000" />
                
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/base_dimen_90"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="@dimen/base_dimen_35"
                	android:layout_marginRight="@dimen/base_dimen_35"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:text="身  份  证"
                        android:textColor="#000000"
                        android:textSize="@dimen/base_dimen_30" />

                    <EditText
                        android:id="@+id/id_rent_house_idcard"
                       android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="#00000000"
                        android:gravity="center_vertical"
                        android:digits="@string/edit_input"
                        android:hint="请输入租房人身份证号码"
                        android:maxLength="18"
                        android:layout_marginLeft="@dimen/base_dimen_50"
                        android:textColor="#444444"
                        android:textColorHint="#b2b2b2"
                        android:textSize="@dimen/base_dimen_30" />
                </LinearLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#0c000000" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/base_dimen_90"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="@dimen/base_dimen_35"
                	android:layout_marginRight="@dimen/base_dimen_35"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:text="门锁密码"
                        android:textColor="#000000"
                        android:textSize="@dimen/base_dimen_30" />


                    <EditText
                        android:id="@+id/door_password"
                       android:layout_width="match_parent"
                        	android:layout_height="match_parent"
                        	android:background="#00000000"
                        	android:gravity="center_vertical"
                        	android:layout_marginLeft="@dimen/base_dimen_50"
                        android:hint="请输入门锁密码（6位数字）"
                        android:inputType="number"
                        android:paddingLeft="5dp"
                        android:maxLength="6"
                        android:textColor="#444444"
                        android:textColorHint="#b2b2b2"
                        android:textSize="@dimen/base_dimen_30" />
                </LinearLayout>
                
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/base_dimen_90"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="@dimen/base_dimen_35"
                	android:layout_marginRight="@dimen/base_dimen_35"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:text="     "
                        android:textColor="#000000"
                        android:textSize="15dp" />

                    <TextView
                        android:layout_width="match_parent"
                        	android:layout_height="match_parent"
                        	android:background="#00000000"
                        	android:gravity="center_vertical"
                        	android:layout_marginLeft="@dimen/base_dimen_50"
                        android:lines="2"
                        android:textColor="#666666"
                        android:text="您可在首页扫码开门，也可输入密码开门入住"
                        android:textSize="@dimen/base_dimen_30" />
                </LinearLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#0c000000" />

                <Button
                    android:id="@+id/id_add_rent_confirm"
                    android:layout_width="@dimen/base_dimen_520"
                    android:layout_height="@dimen/base_dimen_80"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginBottom="@dimen/base_dimen_100"
                    android:layout_marginTop="@dimen/base_dimen_60"
                    android:background="@drawable/item_blue_button_corner_selector"
                    android:gravity="center"
                    android:text="确定"
                    android:textColor="#ffffff"
                    android:textSize="@dimen/base_dimen_30" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

    <include
        android:id="@+id/id_data_loading"
        android:visibility="invisible"
        layout="@layout/layout_loading_dialog" />

    <include
        android:id="@+id/id_qrcode_layout"
        layout="@layout/layout_qrcode_dialog" />

</FrameLayout>