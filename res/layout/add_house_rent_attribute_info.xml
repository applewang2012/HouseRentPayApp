<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical"
    android:paddingLeft="2dp"
    android:paddingRight="2dp"
     >
     <ScrollView 
         android:layout_width="fill_parent"  
         android:layout_height="fill_parent">  
     	<LinearLayout 
             android:layout_width="fill_parent"  
        	 android:layout_height="fill_parent"
        	 android:orientation="vertical">
        	 
             <LinearLayout 
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_marginLeft="20dp"
		        android:layout_marginRight="20dp"
		        android:layout_marginTop="30dp"
		        android:orientation="vertical"
	        	>
	        	
	        	<LinearLayout 
	        	    android:layout_width="match_parent"
		        	android:layout_height="60dp"
		        	android:orientation="horizontal"
		        	android:gravity="center_vertical"
	        	    >
	        	    <TextView 
	        	        android:layout_width="0dp"
		        		android:layout_height="match_parent"
		        		android:layout_weight="1"
		        		android:text="姓	名"
		        		android:textColor="#666666"
			    		android:textSize="15dp"
			    		android:gravity="left|center_vertical"
	        	        />
	        	    <EditText 
	        	        android:id="@+id/id_rent_house_name"
	        	         android:layout_width="0dp"
		        		 android:layout_height="35dp"
		        		 android:layout_weight="3"
		        		 android:hint="请输入租房人姓名"
		        		 android:paddingLeft="5dp"
		        		 android:textSize="14dp"
		        		 android:background="@drawable/item_shape_no_solid_corner"
		        		 android:textColor="#afaeae"
	        	        />
	        	    </LinearLayout>
	        	    <ImageView 
	        	     android:layout_width="match_parent"
		        	 android:layout_height="1dp"
		        	 android:background="#0c000000"
	        	     />
	        	<LinearLayout 
	        	    android:layout_width="match_parent"
		        	android:layout_height="60dp"
		        	android:orientation="horizontal"
		        	android:gravity="center_vertical"
	        	    >
	        	    <TextView 
	        	        android:layout_width="0dp"
		        		android:layout_height="match_parent"
		        		android:layout_weight="1"
		        		android:text="手	机"
		        		android:textColor="#666666"
			    		android:textSize="15dp"
			    		android:gravity="left|center_vertical"
	        	        />
	        	    <EditText 
	        	        android:id="@+id/id_rent_house_phone"
	        	         android:layout_width="0dp"
		        		 android:layout_height="35dp"
		        		 android:layout_weight="3"
		        		 android:hint="请输入租房人手机号码"
		        		 android:maxLength="11"
		        		 android:inputType="number"
		        		 android:paddingLeft="5dp"
		        		 android:textSize="14dp"
		        		 android:background="@drawable/item_shape_no_solid_corner"
		        		 android:textColor="#afaeae"
	        	        />
	        	    </LinearLayout>
	        	<ImageView 
	        	     android:layout_width="match_parent"
		        	 android:layout_height="1dp"
		        	 android:background="#0c000000"
	        	     />
	        	<LinearLayout 
	        	    android:layout_width="match_parent"
		        	android:layout_height="60dp"
		        	android:orientation="horizontal"
		        	android:gravity="center_vertical"
	        	    >
	        	    <TextView 
	        	        android:layout_width="0dp"
		        		android:layout_height="match_parent"
		        		android:layout_weight="1"
		        		android:text="身份证"
		        		android:textColor="#666666"
			    		android:textSize="15dp"
			    		android:gravity="left|center_vertical"
	        	        />
	        	    <EditText 
	        	        android:id="@+id/id_rent_house_idcard"
	        	         android:layout_width="0dp"
		        		 android:layout_height="35dp"
		        		 android:layout_weight="3"
		        		 android:hint="请输入租房人身份证号码"
		        		 android:maxLength="18"
		        		 android:paddingLeft="5dp"
		        		 android:textSize="14dp"
		        		 android:digits="@string/edit_input"
		        		 android:background="@drawable/item_shape_no_solid_corner"
		        		 android:textColor="#afaeae"
	        	        />
	        	    </LinearLayout>
	        	<ImageView 
	        	     android:layout_width="match_parent"
		        	 android:layout_height="1dp"
		        	 android:background="#0c000000"
	        	     />
	        
	        	<LinearLayout 
	        	    android:layout_width="match_parent"
		        	android:layout_height="60dp"
		        	android:orientation="horizontal"
		        	android:gravity="center_vertical"
	        	    >
	        	    <TextView 
	        	        android:layout_width="0dp"
		        		android:layout_height="match_parent"
		        		android:layout_weight="1"
		        		android:text="价	格"
		        		android:textColor="#666666"
			    		android:textSize="15dp"
			    		android:gravity="left|center_vertical"
	        	        />
	        	    <EditText 
	        	        android:id="@+id/id_rent_house_price"
	        	         android:layout_width="0dp"
		        		 android:layout_height="35dp"
		        		 android:layout_weight="3"
		        		 android:hint="请输入和房主约定房屋价格"
		        		 android:gravity="center_vertical"
		        		 android:inputType="number"
		        		 android:maxLength="8"
		        		 android:paddingLeft="5dp"
		        		 android:textSize="14dp"
		        		 android:background="@drawable/item_shape_no_solid_corner"
		        		 android:textColor="#afaeae"
	        	        />
	        	</LinearLayout>
	        	
	        	<ImageView 
	        	     android:layout_width="match_parent"
		        	 android:layout_height="1dp"
		        	 android:background="#0c000000"
	        	     />
	        	<FrameLayout 
	        	    android:id="@+id/id_rent_house_start_date"
	        	    android:layout_width="match_parent"
		        	android:layout_height="60dp"
		        	android:orientation="horizontal"
		        	android:gravity="center_vertical"
		        	android:clickable="true"
		        	android:focusable="true"
	        	    >
	        	    <TextView 
	        	        android:id="@+id/id_rent_house_start_date_text"
	        	        android:layout_width="wrap_content"
		        		android:layout_height="match_parent"
		        		android:layout_weight="2"
		        		android:text="开始日期"
		        		android:textColor="#666666"
			    		android:textSize="15dp"
			    		android:gravity="left|center_vertical"
	        	        />
	        	    <ImageView 
	        	        android:layout_width="8dp"
		        		android:layout_height="12dp"
		        		android:layout_marginRight="10dp"
		        		android:background="@drawable/into_icon"
		        		android:layout_gravity="right|center_vertical"
	        	        />
	        	</FrameLayout>
	        	<ImageView 
	        	     android:layout_width="match_parent"
		        	 android:layout_height="1dp"
		        	 android:background="#0c000000"
	        	     />
	        	<FrameLayout 
	        	    android:id="@+id/id_rent_house_end_date"
	        	    android:layout_width="match_parent"
		        	android:layout_height="60dp"
		        	android:orientation="horizontal"
		        	android:gravity="center_vertical"
		        	android:clickable="true"
		        	android:focusable="true"
	        	    >
	        	    <TextView 
	        	        android:id="@+id/id_rent_house_end_date_text"
	        	        android:layout_width="wrap_content"
		        		android:layout_height="match_parent"
		        		android:layout_weight="2"
		        		android:text="结束日期"
		        		android:textColor="#666666"
			    		android:textSize="15dp"
			    		android:gravity="left|center_vertical"
	        	        />
	        	    <ImageView 
	        	        android:layout_width="8dp"
		        		android:layout_height="12dp"
		        		android:layout_marginRight="10dp"
		        		android:background="@drawable/into_icon"
		        		android:layout_gravity="right|center_vertical"
	        	        />
	        	</FrameLayout>
	        	<ImageView 
	        	     android:layout_width="match_parent"
		        	 android:layout_height="1dp"
		        	 android:background="#0c000000"
	        	     />
	        	<FrameLayout 
	        	    android:id="@+id/id_rent_house_type"
	        	    android:layout_width="match_parent"
		        	android:layout_height="60dp"
		        	android:orientation="horizontal"
		        	android:gravity="center_vertical"
	        	    >
	        	    <TextView 
	        	        android:id="@+id/id_rent_house_type_text"
	        	        android:layout_width="wrap_content"
		        		android:layout_height="match_parent"
		        		android:layout_weight="2"
		        		android:text="房屋类型"
		        		android:textColor="#666666"
			    		android:textSize="15dp"
			    		android:gravity="left|center_vertical"
	        	        />
	        	    <ImageView 
	        	        android:layout_width="8dp"
		        		android:layout_height="12dp"
		        		android:layout_marginRight="10dp"
		        		android:background="@drawable/into_icon"
		        		android:layout_gravity="right|center_vertical"
	        	        />
	        	</FrameLayout>
	        	<ImageView 
	        	     android:layout_width="match_parent"
		        	 android:layout_height="1dp"
		        	 android:background="#0c000000"
	        	     />
	         </LinearLayout>
             <Button 
                 android:id="@+id/id_add_rent_confirm"
                 android:layout_width="@dimen/base_dimen_520"
		         android:layout_height="@dimen/base_dimen_80"
		         android:layout_marginTop="@dimen/base_dimen_60"
		         android:layout_marginBottom="@dimen/base_dimen_100"
		         android:text="确   定"
		         android:textColor="@drawable/button_text_selector"
		         android:textSize="@dimen/base_dimen_30"
		         android:layout_gravity="center_horizontal"
		         android:gravity="center"
		         android:background="@drawable/button_background_selector"
                 />
	       </LinearLayout>
     </ScrollView>

    <include
        android:id="@+id/id_data_loading"
        layout="@layout/layout_loading_dialog"
       />
    
     <include
        android:id="@+id/id_qrcode_layout"
        layout="@layout/layout_qrcode_dialog"
       />
	       
</FrameLayout>
