                            android:singleLine="true"
                			android:ellipsize="marquee"
                			android:marqueeRepeatLimit="marquee_forever"
                            android:textSize="@dimen/base_dimen_30" />
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#0c000000" />
                
                <FrameLayout
                    android:id="@+id/id_rent_house_start_date"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/base_dimen_90"
                    android:clickable="true"
                    android:focusable="true"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/base_dimen_90"
                        android:layout_marginLeft="@dimen/base_dimen_35"
                		android:layout_marginRight="@dimen/base_dimen_35"
                        android:gravity="center_vertical"
                        android:orientation="horizontal" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="left|center_vertical"
                            android:text="入住时间"
                            android:textColor="#000000"
                            android:textSize="@dimen/base_dimen_30" />

                        <TextView
                            android:id="@+id/id_rent_house_start_date_text"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="@dimen/base_dimen_50"
                            android:gravity="center"
                            android:textColor="#444444"
                            android:textSize="@dimen/base_dimen_30" />
                    </LinearLayout>

                    <ImageView
                        android:layout_width="8dp"
                        android:layout_height="12dp"
                        android:layout_gravity="right|center_vertical"
                        android:layout_marginRight="@dimen/base_dimen_35"
                        android:background="@drawable/into_icon" />
                </FrameLayout>


                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#0c000000" />

                 <FrameLayout
                    android:id="@+id/id_rent_house_end_date"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/base_dimen_90"
                    android:clickable="true"
                    android:focusable="true"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/base_dimen_90"
                        android:layout_marginLeft="@dimen/base_dimen_35"
                		android:layout_marginRight="@dimen/base_dimen_35"
                        android:gravity="center_vertical"
                        android:orientation="horizontal" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="left|center_vertical"
                            android:text="退房时间"
                            android:textColor="#000000"
                            android:textSize="@dimen/base_dimen_30" />

                        <TextView
                            android:id="@+id/id_rent_house_end_date_text"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="@dimen/base_dimen_50"
                            android:gravity="center"
                            android:textColor="#444444"
                            android:textSize="@dimen/base_dimen_30" />
                    </LinearLayout>

                    <ImageView
                        android:layout_width="8dp"
                        android:layout_height="12dp"
                        android:layout_gravity="right|center_vertical"
                        android:layout_marginRight="@dimen/base_dimen_35"
                        android:background="@drawable/into_icon" />
                </FrameLayout>
                
                
                <Button
                    android:id="@+id/id_check_can_rent_house"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|right"
                    android:layout_marginRight="@dimen/base_dimen_45"
                    android:layout_marginBottom="@dimen/base_dimen_10"
                    android:background="#00000000"
                    android:gravity="center"
                    android:text="查看已出租时间列表"
                    android:textColor="#337ffd"
                    android:textSize="15dp" />
                
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#0c000000" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/base_dimen_90"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="@dimen/base_dimen_35"
                	android:layout_marginRight="@dimen/base_dimen_35"
                    android:orientation="horizontal" >
                    
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="left|center_vertical"
                            android:text="房屋价格"
                            android:textColor="#000000"
                            android:textSize="@dimen/base_dimen_30" />


                    <TextView
                        android:id="@+id/id_rent_house_price"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:background="#00000000"
                        android:gravity="center_vertical"
                        android:hint="请输入和房主约定房屋价格"
                        android:layout_marginLeft="@dimen/base_dimen_50"
                        android:textColor="#444444"
                        android:textColorHint="#b2b2b2"
                        android:textSize="@dimen/base_dimen_30" />
                </LinearLayout>


