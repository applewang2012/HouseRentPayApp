                android:layout_height="wrap_content"
               
                android:layout_marginTop="10dp"
                android:orientation="vertical" >

                <FrameLayout
                    android:id="@+id/id_rent_house_type"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/base_dimen_90"
                    android:clickable="true"
                        android:focusable="true"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/base_dimen_90"
                        
                         android:layout_marginLeft="@dimen/base_dimen_35"
                		android:layout_marginRight="@dimen/base_dimen_35"
                        android:gravity="center_vertical"
                        android:orientation="horizontal" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="left|center_vertical"
                            android:text="租赁类型"
                            android:textColor="#000000"
                            android:textSize="@dimen/base_dimen_30" />

                        <TextView
                            android:id="@+id/id_rent_house_type_text"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="@dimen/base_dimen_50"
                            android:gravity="center"
                            android:textColor="#444444"
                            android:textSize="@dimen/base_dimen_30" />
                    </LinearLayout>

                    <!-- <ImageView
                        android:layout_width="8dp"
                        android:layout_height="12dp"
                        android:layout_gravity="right|center_vertical"
                        android:layout_marginRight="@dimen/base_dimen_35"
                        android:background="@drawable/into_icon" /> -->
                </FrameLayout>
            
				<TextView 
				    android:id="@+id/id_rent_house_address_text"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/base_dimen_90"
                            android:layout_marginLeft="@dimen/base_dimen_35"
                			android:layout_marginRight="@dimen/base_dimen_35"
                            android:gravity="center"
                            android:textColor="#000000"
